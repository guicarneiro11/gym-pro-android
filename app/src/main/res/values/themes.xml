<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.GymPro" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
    </style>
    <style name="Theme.GymPro.Starting" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">#1F1F1F</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/splash_icon</item>
        <item name="windowSplashScreenAnimationDuration">300</item>
        <item name="postSplashScreenTheme">@style/Theme.GymPro</item>
        <item name="android:windowBackground">#1F1F1F</item>
        <item name="android:statusBarColor">#1F1F1F</item>
        <item name="android:navigationBarColor">#1F1F1F</item>
    </style>
</resources>